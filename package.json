{
  "name": "fluster",
  "author": "Fluster",
  "homepage": "https://fluster.io",
  "version": "5.0.0-rc.0",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "build": "ionic build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pwa": "gulp rmmockup && gulp pwa && ionic build --prod --project pwa",
    "diagram": "./diagram.sh",
    "docs": "typedoc --out ./docs ./src/app/services/**/*.ts --module commonjs --umlFormat svg --umlLocation local --umlSeperateClassDetailView true --umlComplete detail",
    "pwa-staging": "gulp rmmockup && gulp staging && gulp pwa && ionic build --prod --project pwa -c staging"
  },
